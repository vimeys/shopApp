.title{
    display: flex;
    flex: 1;
    flex-direction: column;
    justify-content: space-between;
    margin-left: 20rpx;
    margin-right:20rpx ;
    margin-top: 6rpx;
    border-bottom: 1px solid #eee;
    font-size: 36rpx;
}
.title .promotion_main{
    width: 430rpx;
    white-space:nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
    font-size: 32rpx;
}
.title .promotion_scale{
    font-size: 28rpx;
    color:#ff9388;
}

.promotion_price{
    color:#EA4736;
    font-size: 32rpx;
}
.promotion_price_del{
    color:#989898;
    font-size: 22rpx;
    margin-left: 5rpx;
    text-decoration: line-through;
}
.promotion_num{
    color:#989898;
    font-size: 22rpx;
    float: right;
}
.TbD_people{
    font-size:32rpx;
}
