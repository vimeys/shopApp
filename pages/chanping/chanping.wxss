/* pages/chanping/chanping.wxss */
.chanping{
  margin-bottom: 100rpx;
}
.chanping-top{
    line-height: 88rpx;
    border-bottom: 10rpx #eee solid;
    font-size: 36rpx;
    display: flex;
    justify-content: space-between;
}
.chanping-top-left{
    color: #EA4736;
    padding-left: 20rpx;
    display: flex;
}
.chanping-top-right{
    padding-right: 20rpx;
    display: flex;
}
.chanping-title{
    line-height: 60rpx;
    border-bottom: 2rpx #eee solid;
    font-size: 32rpx;
    display: flex;
}
.chanping-title>view{
    padding: 0 30rpx 0 30rpx;
    flex:1;
}
.xuanzhong{
    color: #EA4736;
}

